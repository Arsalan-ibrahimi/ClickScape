{
    "manifest_version": 3,
  
    "name": "ClickScape",
    "description": "A Chrome extension for URL access and quick navigation on click",
    "version": "1.0",
  
    "action": {
    
      "default_popup": "popup/popup.html",
      
      "default_title": "Instant Tabs!",
    "default_icon": "assets/images/logo/logo.png"
     
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
